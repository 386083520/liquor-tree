{
  "name": "liquor-tree",
  "description": "A Vue.js tree component.",
  "version": "0.2.15",
  "author": "Kostiantyn <phlyze@gmail.com>",
  "library": "LiquorTree",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -cw rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "lint": "eslint --fix src",
    "test": "npm run lint && npm run unit",
    "unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amsik/liquor-tree.git"
  },
  "module": "dist/liquor-tree.esm.js",
  "main": "dist/liquor-tree.umd.js",
  "keywords": [
    "vue",
    "tree",
    "component",
    "vue-tree",
    "treeview"
  ],
  "dependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.19.1",
    "eslint-plugin-vue-libs": "^2.1.0",
    "jest": "^22.4.3",
    "rollup": "^0.55.5",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.19.1",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "vue-jest": "^2.3.0",
    "vue-template-compiler": "^2.5.13"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  }
}
